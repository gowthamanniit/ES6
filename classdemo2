        {
        
        class Student
        {            
            constructor()
            {
                this.rno=1001;
                this.name='raja';
                alert("test");
            }            
             dis() {
                //document.write(this.rno);
                //document.write(this.name);
                return 'roll no:'+this.rno+' name = '+this.name;
            }
        }
        s1=new Student(); // automatic arg constructor        
        ans=s1.dis(); // user call manual
        alert(ans);
        document.getElementById("root").innerHTML=ans;
        }
    </script>

